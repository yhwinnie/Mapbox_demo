# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'Mapbox_demo' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Mapbox_demo
	pod 'Mapbox-iOS-SDK', '~> 3.2.3'
	pod 'MapboxGeocoder.swift', :git => 'https://github.com/mapbox/MapboxGeocoder.swift.git', :tag => 'v0.5.0'
	pod 'MapboxDirections.swift', :git => 'https://github.com/mapbox/MapboxDirections.swift.git', :tag => 'v0.6.0'
	  pod 'SwiftyJSON'
  	pod 'Alamofire'
  	pod 'AlamofireImage'
  	pod 'AlamofireNetworkActivityIndicator'
	pod 'ACTabScrollView', :git => 'https://github.com/azurechen/ACTabScrollView.git'
	pod 'Firebase', '>=2.4.2'
	pod 'Firebase/Database'
	pod 'Firebase/Auth'
   







end
